local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("MAMBA HUB", "Ocean")

-- MAIN
local Tab = Window:NewTab("Main")
local MainSection = Tab:NewSection("Main")
local Tab = Window:NewTab("AutoFarm")
local MacSection = Tab:NewSection("Auto Clicker")
local FheSection = Tab:NewSection("Free Hatch Eggs")

MainSection:NewSlider("WalkSpeed", "Walk Faster", 500, 16, function(s) -- 500 (MaxValue) | 0 (MinValue)
    game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = s
end)

MacSection:NewButton("Auto Clicker Mega Click", "Turn on Auto Clicker for Mega Click with out CD", function()
 	while wait(.1) do
 	game.ReplicatedStorage.MegaClicks:FireServer()
end)

FheSection:NewButton("Halloween Egg", "Free Hatch Halloween Egg", function()
 	game.ReplicatedStorage.ServerEvents.hatchEgg:FireServer(0, workspace.EventEggs.HalloweenEgg, "Single")
end)

FheSection:NewButton("Halloween Egg 2", "Free Hatch Halloween Egg 2", function()
 	game.ReplicatedStorage.ServerEvents.hatchEgg:FireServer(0, workspace.EventEggs.HalloweenEgg2, "Single")
end)
