local key = script.Parent.Parent.putcode
local getkey = script.Parent.Parent
local mac = script.Parent.Parent.Parent.MCS
local prl = script.Parent.Parent.Parent.PRL
local redeem = script.Parent

redeem.MouseButton1Click:Connect(function()
	if key.Text == 'Hs8f9H*$3984HFH493hf43hfusIHf443pfh8hiusfuyir' then
		if game.PlaceId == 11142132921 then
			getkey.Visible = false
			wait(0.5)
			mac.Visible = true
		end
		if game.PlaceId == 155615604 then
			getkey.Visible = false
			wait(0.5)
			prl.Visible = true
		end
	end
end)
end))
